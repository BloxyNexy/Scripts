local a=loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()local b=a:MakeWindow({Name="DizzyCheats",HidePremium=false,SaveConfig=true,ConfigFolder="DizzyCheats",IntroText="DizzyCheats"})local c=""local d=false local e=true local f=game:GetService("Players") local g=game:GetService("RunService") local h=game:GetService("UserInputService") local i=f.LocalPlayer local j=false local k=Color3.fromRGB(255,0,0) local l=Color3.fromRGB(0,0,0) local m=nil local n=Instance.new("Folder") n.Parent=game.Workspace n.Name="DizzyCheats" local function o() if d==true and e==true then local p=game:GetService("HttpService") local q=game:GetService("StarterGui") local r="Dizzy Verificaition" q:SetCore("SendNotification",{Title=r,Text="Intializing Authentication...",Duration=5}) local s=false local t="" local u="Dizzy Cheats" local v="zq6rLuzXEx" local w="1.0" local x=game:HttpGet("https://keyauth.win/api/1.1/?name="..u.."&ownerid="..v.."&type=init&ver="..w) if x=="KeyAuth_Invalid" then print(" Error: Application not found.") q:SetCore("SendNotification",{Title=r,Text=" Error: Application not found.",Duration=3}) return false end local y=p:JSONDecode(x) if y.success==true then s=true t=y.sessionid elseif(y.message=="invalidver")then print(" Error: Wrong application version..") q:SetCore("SendNotification",{Title=r,Text=" Error: Wrong application version..",Duration=3}) return false else print(" Error: "..y.message) return false end print("\n\n Licensing... \n") local x=game:HttpGet('https://keyauth.win/api/1.1/?name='..u..'&ownerid='..v..'&type=license&key='..c..'&ver='..w..'&sessionid='..t) local y=p:JSONDecode(x) d=false if y.success==false then q:SetCore("SendNotification",{Title=r,Text=" Error: "..y.message,Duration=5}) return false end q:SetCore("SendNotification",{Title=r,Text=" Successfully Authorized :)",Duration=5}) e=false end end local z=b:MakeTab({Name="Log in",Icon="",PremiumOnly=false}) local aa=z:AddSection({Name="Key system"}) aa:AddTextbox({Name="Key",Default="",TextDisappear=true,Callback=function(ab) c=ab print(c) end}) aa:AddButton({Name="Check key",Callback=function() d=true o() end}) local function ac(ad) if ad and not ad:FindFirstChild("Highlight")then local ae=Instance.new("Highlight") ae.Parent=ad ae.AlwaysOnTop=true ae.FillColor=k ae.OutlineColor=l end end local function af(ad) if ad then local ae=ad:FindFirstChild("Highlight")if ae then ae:Destroy()end end end local function ag() for _,ah in pairs(f:GetPlayers())do if ah~=i then local ai=game.Workspace:FindFirstChild(ah.Name) if ai then if j then ac(ai)else af(ai)end end end end end local function aj() if e==false then j=not j ag() end end for _,ah in pairs(f:GetPlayers())do if ah~=i then ah.CharacterAdded:Connect(function(ai) if j then ac(ai)end end) end end g.RenderStepped:Connect(function() if j==true and e==false then wait() ag() end end) local ak=b:MakeTab({Name="Visual",Icon="",PremiumOnly=false}) local al=ak:AddSection({Name="ESP"}) local am=al:AddToggle({Name="Highlights",Default=false,Callback=function(an) if e==false then j=an ag()else a:MakeNotification({Name="not logged in!",Content="Get a key and log in first bozo",Time=5})end end}) local ao=b:MakeTab({Name="Misc",Icon="",PremiumOnly=false}) local ap=ao:AddSection({Name="Misc"}) ap:AddButton({Name="Inf Yield (press once and wait 5 seconds)",Callback=function() a:MakeNotification({Name="Executing Inf Yield",Content="Wait a few seconds as we are starting up inf yield",Image="",Time=5}) if e==false then loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()else a:MakeNotification({Name="not logged in!",Content="Get a key and log in first bozo",Time=5})end end}) local aq=b:MakeTab({Name="Binds",Icon="",PremiumOnly=false}) aq:AddBind({Name="ToggleHighLights",Default=Enum.KeyCode.RightShift,Hold=false,Callback=function() if e==false then j=not j ag()else a:MakeNotification({Name="not logged in!",Content="Get a key and log in first bozo",Time=5})end end}) a:MakeNotification({Name="Loaded!",Content="Enjoy DizzyCheats V1",Time=5}) a:Init()
