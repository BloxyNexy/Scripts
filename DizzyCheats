local a=loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()local b=a:MakeWindow({Name="DizzyCheats",HidePremium=false,SaveConfig=true,ConfigFolder="DizzyCheats",IntroText="DizzyCheats"})local c=""local d=false local e=true local f=false local g=game:GetService("Players")local h=game:GetService("RunService")local i=game:GetService("UserInputService")local j=g.LocalPlayer local k=true local l=Color3.fromRGB(255,0,0)local m=Color3.fromRGB(0,0,0)local n=nil local o=Instance.new("Folder")o.Parent=game.Workspace o.Name="DizzyCheats"local function p()if d==true and e==true then print(' KeyAuth Lua Example - https://github.com/mazk5145/')local q=game:GetService("HttpService")local r=game:GetService("StarterGui")local s="KeyAuth Lua Example"r:SetCore("SendNotification",{Title=s,Text="Intializing Authentication...",Duration=5})local t=false local u=""local v="ewf";local w="zq6rLuzXEx";local x="1.0";local y=game:HttpGet("https://keyauth.win/api/1.1/?name="..v.."&ownerid="..w.."&type=init&ver="..x)if y=="KeyAuth_Invalid"then print(" Error: Application not found.")r:SetCore("SendNotification",{Title=s,Text=" Error: Application not found.",Duration=3})return false end local z=q:JSONDecode(y)if z.success==true then t=true u=z.sessionid elseif(z.message=="invalidver")then print(" Error: Wrong application version..")r:SetCore("SendNotification",{Title=s,Text=" Error: Wrong application version..",Duration=3})return false else print(" Error: "..z.message)return false end print("\n\n Licensing... \n")local y=game:HttpGet('https://keyauth.win/api/1.1/?name='..v..'&ownerid='..w..'&type=license&key='..c..'&ver='..x..'&sessionid='..u)local z=q:JSONDecode(y)d=false if z.success==false then r:SetCore("SendNotification",{Title=s,Text=" Error: "..z.message,Duration=5})return false end r:SetCore("SendNotification",{Title=s,Text=" Successfully Authorized :)",Duration=5})e=false f=true end end local A=b:MakeTab({Name="Log in",Icon="",PremiumOnly=false})local B=A:AddSection({Name="Key system"})B:AddTextbox({Name="Key",Default="",TextDisappear=true,Callback=function(C)c=C print(c)end})B:AddButton({Name="Check key",Callback=function()d=true p()end})if f==true then local function D(E)if E and not E:FindFirstChild("Highlight")then local F=Instance.new("Highlight")F.Parent=E F.AlwaysOnTop=true F.FillColor=l F.OutlineColor=m end end local function G(E)if E then local F=E:FindFirstChild("Highlight")if F then F:Destroy()end end end local function H()for _,I in pairs(g:GetPlayers())do if I~=j then local E=game.Workspace:FindFirstChild(I.Name)if E then if k then D(E)else G(E)end end end end end local function J()k=not k H()end for _,I in pairs(g:GetPlayers())do if I~=j then I.CharacterAdded:Connect(function(E)if k then D(E)end end)end end h.RenderStepped:Connect(function()if k then wait()H()end end)local K=b:MakeTab({Name="Visual",Icon="",PremiumOnly=false})local L=K:AddSection({Name="ESP"})local M=L:AddToggle({Name="Highlights",Default=false,Callback=function(N)k=N H()end})local O=b:MakeTab({Name="Misc",Icon="",PremiumOnly=false})local P=O:AddSection({Name="Misc"})P:AddButton({Name="Inf Yield (press once and wait 5 seconds)",Callback=function()a:MakeNotification({Name="Executing Inf Yield",Content="Wait a few seconds as we are starting up inf yield",Image="",Time=5})loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()end})local Q=b:MakeTab({Name="Binds",Icon="",PremiumOnly=false})Q:AddBind({Name="ToggleHighLights",Default=Enum.KeyCode.RightShift,Hold=false,Callback=function()k=not k H()end})a:MakeNotification({Name="Loaded!",Content="Enjoy DizzyCheats V1",Time=5})end a:Init()
